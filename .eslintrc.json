{
  "root": true,
  "env": {
    "amd": true,
    "browser": true
  },
  "rules": {
    "no-alert": "off",
    "no-new": "off",
    "key-spacing": "off",
    "space-before-function-paren": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "comma-dangle": ["error", "never"],
    "semi": 0
  },
  "extends": "es3",
  "globals": {
    "$": "readonly",
    "requirejs": "readonly",
    "jquery": "readonly",
    "define": "readonly",
    "ActiveXObject": "readonly"
  }
}
